{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "content-type": "text/javascript",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "content-type": "text/javascript"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://fonts.gstatic.com; font-src 'self' https://fonts.gstatic.com data:;"
  }
}