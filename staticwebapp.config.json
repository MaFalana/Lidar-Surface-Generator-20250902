{
  "routes": [
    {
      "route": "/breakline-gen/assets/*.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/breakline-gen/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/breakline-gen/assets/*.css",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/breakline-gen",
      "rewrite": "/index.html"
    },
    {
      "route": "/breakline-gen/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/breakline-gen/assets/*", "/breakline-gen/*.js", "/breakline-gen/*.css", "/*.png", "/*.jpg", "/*.jpeg", "/*.svg", "/*.ico"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://fonts.gstatic.com; font-src 'self' https://fonts.gstatic.com data:;"
  }
}